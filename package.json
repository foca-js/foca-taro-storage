{
  "name": "foca-taro-storage",
  "version": "1.0.3",
  "repository": "git@github.com:foca-js/foca-taro-storage.git",
  "keywords": [
    "redux-persist",
    "taro-persist",
    "taro-storage"
  ],
  "description": "Foca持久化taro引擎",
  "contributors": [
    "罪 <fanwenhua1990@gmail.com> (https://github.com/geekact)"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": true,
  "scripts": {
    "prepublishOnly": "tsup",
    "prepare": "husky install"
