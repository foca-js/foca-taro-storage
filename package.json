{
  "name": "foca-taro-storage",
  "version": "1.0.0",
  "repository": "git@github.com:foca-js/foca-taro-storage.git",
  "keywords": [
    "taro-persist",
    "taro-storage"
  ],
  "description": "Foca持久化taro引擎",
