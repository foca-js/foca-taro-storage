      "@commitlint/config-conventional"
    ]
  },
  "peerDependencies": {
    "@tarojs/taro": "^3.0.0",
    "foca": ">=0.11.0",
    "react": ">=16.9.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@swc/core": "^1.3.23",
    "@tarojs/taro": "^3.5.10",
    "@types/node": "^18.11.17",
    "foca": "^1.3.0",
    "husky": "^8.0.2",
    "prettier": "^2.8.1",
    "react": "^18.2.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  }
