    "@commitlint/config-conventional": "^17.0.2",
    "@swc/core": "^1.2.197",
    "@tarojs/taro": "^3.4.11",
    "@types/node": "^17.0.41",
    "browserslist": "^4.20.4",
    "foca": "^0.12.2",
    "husky": "^8.0.1",
    "prettier": "^2.6.2",
